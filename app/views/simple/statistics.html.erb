<br>
<div class="container">
<h3>Statistics</h3>
<hr>


	<table class="table table-striped">
		<tr>
			<th>Statistics Type</th>
			<th>Count</th>
		</tr>

		<tr>
			<td>Number of amino acid sequences in database</td>
			<td class="stats-num"><%= @number_of_cus_aa_sequence %></td>
		</tr>

		<tr>
			<td>Number Of RD Groups</td>
			<td class="stats-num"><%= @number_of_group %></td>
		</tr>

		<tr>
			<td>Number Of RD Organisms</td>
			<td class="stats-num"><%= @number_of_organism %></td>
		</tr>

		<tr>
			<td> Number of Compounds</td>
			<td class="stats-num"> <%= @number_of_compound %> </td>
		</tr>

	</table>

</div>
